#ALIAS {questrun}
{
     #path load ${path$placetable[$target[place]][path]};
     #variable pathrun 1;
     #variable pathend 0;
     rideto $placetable[$target[place]][id];
     #map goto $placetable[$target[place]][vnum];
     #switch {"$quest[type]"}
     {
          #case {"letter"} {#path run 0.3};
          #case {"quest"} {#path run 0.3};
          #case {"rumor"} {#path run 0.3};
          #case {"map"} {map here};
     }
}

#ALIAS {start}
