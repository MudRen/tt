#ALIAS {mapoff}
{
     #split
}

#ALIAS {mapon}
{
     #split 0 1 0 -80;
     #map offset 1 81 -4 -1;
     #map flag vtmap on;
     #map flag static on
}

#ALIAS {mapping}
{
     #map flag static off;
     #variable mapset 1
}

#ALIAS {mdn}
{
     #map dig %1 new
}

#ALIAS {mf}
{
     #if {"%1"==""}
     {
          #echo 请输入目的地名称或编号!
     };
     #else
     {
          #map find %1
     }
}

#ALIAS {mgo}
{
     #if {"%1"==""}
     {
          #echo 请输入目的地名称或编号!
     };
     #else
     {
          #map find %1;
          #path run 0.5
     }
}

#ALIAS {mi}
{
     #map info
}

#ALIAS {mrun}
{
     #if {"%1"==""}
     {
          #echo 请输入目的地名称或编号!
     };
     #else
     {
          #map run %1
     }
}

#ALIAS {pathend}
{
     chat* sweat
}

#ALIAS {patherror}
{
     chat* stomachache
}

#ALIAS {river}
{
     cross
}

#ALIAS {rma}
{
     #map set roomarea %1
}

#ALIAS {rmn}
{
     #map set roomname %1
}

#ALIAS {rms}
{
     #map set roomsymbol %1
}

#ALIAS {savemap}
{
     #map write world.map;
