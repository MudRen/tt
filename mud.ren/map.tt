#ALIAS {gps}
{
     #if {"%1"==""}
     {
          #echo 请输入目的地名称或编号!
     };
     #else
     {
          #map list %1
     }
}

#ALIAS {initmap}
{
     #echo 载入地图系统...;
     loadmap;
     #echo 初始化地图数据...;
     #if {"%1"==""}
     {
          #map goto 1
     };
     #else
     {
          #map goto %1
     };
     mapon
}

#ALIAS {loadmap}
{
     #map read world.map
}

#ALIAS {maphelp}
{
     #echo {-== 炎黄地图导航系统 ==-};
     #echo {使用方法: 到达扬州中央广场后输入 initmap 载入地图并初始化位置};
     #echo {地点查询：输入“gps 地名|vnum”可查询指定地点的vnum和距离};
     #echo {自动寻路：输入“mgo 地名|vnum”可快速到达最近的目的地}
}

